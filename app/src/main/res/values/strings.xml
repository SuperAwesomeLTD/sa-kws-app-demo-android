    <!-- permissions -->
    <string name="page_features_row_perm_title">Manage permissions</string>
    <string name="page_features_row_perm_content">Kids Web Services handles the complexities of dealing with personally identifiable information for. It allows you to request permission from a parent when requesting such data.</string>
