    <!-- platform -->
    <string name="platform_title">Welcome to your\nKWS Demo App</string>
    <string name="platform_content">This KWS Demo App is powered by Kids Web Services (KWS). KWS is a platform that provides a set of compliance-as-a-service tools to create a kid-safe, COPPA-compliant sites and apps; KWS allows you to focus on delivering awesome content, by handling all the legal complexity related to data-collection in the kids space in one simple, customizable package.</string>
    <string name="platform_feature_1">Verifiable\nparental\nconsent</string>
    <string name="platform_feature_2">Loyalty\nplatform\nfor kids</string>
    <string name="platform_feature_3">Federated\nIdentity</string>
    <string name="platform_feature_4">Parent\nPortal</string>
    <string name="platform_main_button">KNOW MORE ABOUT KIDS WEB SERVICES</string>

    <!-- features -->
    <string name="feature_cell_auth_title">Secure single sign-on</string>
    <string name="feature_cell_auth_content">Kids Web Services\' federated identity system (Single Sign-On) allows kids and parents to create one secure login to access your collection of kid-safe apps and sites.</string>
    <string name="feature_cell_auth_button_1_loggedout">AUTHENTICATE USER</string>
    <string name="feature_cell_auth_button_1_loggedin">LOGGED IN AS</string>
    <string name="feature_cell_auth_button_2">SEE DOCUMENTATION</string>
    <string name="feature_cell_notif_title">Kid-safe push notifications</string>
    <string name="feature_cell_notif_content">Safely manage Push Notifications for your users through Kids Web Services. Enable, disable and schedule notifications to be sent to your user base on a regular basis.</string>

    <!-- Fuzzy -->
    <string name="feature_cell_notif_button_1_enable">ENABLE PUSH NOTIFICATIONS</string>
    <string name="feature_cell_notif_button_2">SEE DOCUMENTATION</string>
    <string name="feature_cell_perm_title">Manage permissions</string>
    <string name="feature_cell_perm_content">Kids Web Services handles the complexities of dealing with personally identifiable information for. It allows you to request permission from a parent when requesting such data.</string>
    <string name="feature_cell_perm_button_1">ADD PERMISSIONS</string>
    <string name="feature_cell_perm_button_2">SEE DOCUMENTATION</string>
    <string name="feature_cell_evt_title">Events and points</string>
    <string name="feature_cell_evt_message">Kids Web Services can handle a whole multi-currency system for you. You can add or subtract points for each user, check latest scores or manage leaderboards.</string>
    <string name="feature_cell_evt_button_1">ADD 20 POINTS</string>
    <string name="feature_cell_evt_button_2">SUBTRACT 10 POINTS</string>
    <string name="feature_cell_evt_button_3">GET SCORE</string>
    <string name="feature_cell_evt_button_4">SEE LEADERBOARD</string>
    <string name="feature_cell_evt_button_5">SEE DOCUMENTATION</string>
    <string name="feature_cell_inv_title">Invite friends</string>
    <string name="feature_cell_inv_message">You can invite players to your app by using their email. KWS will ensure that everything is compliant.</string>
    <string name="feature_cell_inv_button_1">INVITE FRIEND</string>
    <string name="feature_cell_inv_button_2">SEE DOCUMENTATION</string>
    <string name="feature_cell_appd_title">App data</string>
    <string name="feature_cell_appd_message">Set custom app data for each player in an app through a set of name-value pairs.</string>
    <string name="feature_cell_appd_button_1">SEE APP DATA</string>
    <string name="feature_cell_appd_button_2">SEE DOCUMENTATION</string>
