    <string name="page_features_row_notif_popup_reg_error_network_message">A network error occurred trying to subscribe to Push Notifications. Please try again!</string>

    <string name="page_features_row_notif_popup_unreg_success_title">Awesome!</string>
    <string name="page_features_row_notif_popup_unreg_success_message">You have successfully un-registered for Remote Notifications in KWS!</string>

