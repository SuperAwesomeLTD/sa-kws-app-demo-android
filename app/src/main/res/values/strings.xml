    <!-- *******************************************************************************************
     Platform
     ******************************************************************************************* -->
    <string name="page_platform_title">Platform</string>
    <string name="page_platform_text_welcome">Welcome to your\nKWS App</string>
    <string name="page_platform_text_content">This KWS App is powered by Kids Web Services (KWS). KWS is a platform that provides a set of compliance-as-a-service tools to create a kid-safe, COPPA-compliant sites and apps; KWS allows you to focus on delivering awesome content, by handling all the legal complexity related to data-collection in the kids space in one simple, customizable package.</string>
    <string name="page_platform_text_feature_1">Verifiable\nparental\nconsent</string>
    <string name="page_platform_text_feature_2">Loyalty\nplatform\nfor kids</string>
    <string name="page_platform_text_feature_3">Federated\nIdentity</string>
    <string name="page_platform_text_feature_4">Parent\nPortal</string>
    <string name="page_platform_button_more">KNOW MORE ABOUT KIDS WEB SERVICES</string>

    <!-- *******************************************************************************************
     Features
     ******************************************************************************************* -->
    <string name="page_features_title">Features</string>

    <!-- auth -->
    <string name="page_features_row_auth_title">Secure single sign-on</string>
    <string name="page_features_row_auth_content">Kids Web Services\' federated identity system (Single Sign-On) allows kids and parents to create one secure login to access your collection of kid-safe apps and sites.</string>
    <string name="page_features_row_auth_button_login_not_logged">AUTHENTICATE USER</string>
    <string name="page_features_row_auth_button_login_logged">LOGGED IN AS</string>
    <string name="page_features_row_auth_button_doc">SEE DOCUMENTATION</string>

    <!-- notififcations -->
    <string name="page_features_row_notif_title">Push notifications</string>
    <string name="page_features_row_notif_content">Safely manage Kid-Safe Push Notifications for your users through Kids Web Services. Enable, disable and schedule notifications to be sent to your user base on a regular basis.</string>
    <string name="page_features_row_notif_button_enable">ENABLE PUSH NOTIFICATIONS</string>
    <string name="page_features_row_notif_button_disable">DISABLE PUSH NOTIFICATIONS</string>
    <string name="page_features_row_notif_button_doc">SEE DOCUMENTATION</string>

    <string name="page_features_row_notif_popup_reg_success_title">Great!</string>
    <string name="page_features_row_notif_popup_reg_success_message">You\'ve successfully registered for Remote Notifications.</string>

    <string name="page_features_row_notif_popup_reg_error_disable_parent_title">Oops - something went wrong!</string>
    <string name="page_features_row_notif_popup_reg_error_disable_parent_message">The parent appears to have disabled Remote Notifications for this user.</string>

    <string name="page_features_row_notif_popup_reg_error_disable_user_title">Oops - something went wrong!</string>
    <string name="page_features_row_notif_popup_reg_error_disable_user_message">The user appears to have disabeld Remote Notifications for his device for this app.</string>

    <string name="page_features_row_notif_popup_reg_error_no_email_title">Oops - something went wrong!</string>
    <string name="page_features_row_notif_popup_reg_error_no_email_message">This user does not appear to have a Parent Email associated with the account.</string>

    <string name="page_features_row_notif_popup_reg_error_firebase_not_setup_title">Oops - something went wrong!</string>
    <string name="page_features_row_notif_popup_reg_error_firebase_not_setup_message">Firebase does not seem to be setup correctly.</string>

    <string name="page_features_row_notif_popup_reg_error_firebase_nil_token_title">Oops - something went wrong!</string>
    <string name="page_features_row_notif_popup_reg_error_firebase_nil_token_message">Firebase could not get the notification token.</string>

